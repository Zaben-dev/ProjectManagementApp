{
  "extends": "airbnb-typescript-prettier",
  "rules": {
    "import/prefer-default-export": "off",
    "@typescript-eslint/no-namespace": "off",
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
